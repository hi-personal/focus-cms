{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "istvan1243/focus",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bacon/bacon-qr-code": "^3.0",
        "barryvdh/laravel-debugbar": "^3.14",
        "composer/installers": "^2.3",
        "devanych/mime-types": "^2.1",
        "embed/embed": "^4.4",
        "erusev/parsedown-extra": "^0.8.1",
        "intervention/image": "^3.11",
        "jenssegers/agent": "^2.6",
        "kitloong/laravel-migrations-generator": "^7.0",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.9",
        "league/commonmark": "^2.6",
        "mailjet/laravel-mailjet": "^3.1",
        "mews/purifier": "^3.4",
        "paragonie/constant_time_encoding": "^3.0",
        "pragmarx/google2fa-laravel": "^2.3",
        "propaganistas/laravel-phone": "^5.3",
        "robthree/twofactorauth": "^3.0",
        "spomky-labs/otphp": "^11.3",
        "symfony/process": "^7.2",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.3",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hi-personal/focus-cms-cti.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/hi-personal/focus-cms-cmi.git",
            "no-api": true
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/MarkdownHelper.php",
            "app/Helpers/vite.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Themes\\": "Themes/"
        },
        "files": [
            "app/Helpers/MarkdownHelper.php",
            "app/Helpers/UserAgentHelper.php"
        ]
    },
    "scripts": {
        "post-package-install": [

        ],
        "post-package-uninstall": [

        ],
        "post-install-cmd": [

        ],
        "post-update-cmd": [

        ],
        "post-require-cmd": [

        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "themes.json",
                "modules.json"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "focus-cms/composer-focus-module-installer": true,
            "focus-cms/composer-focus-theme-installer": true,
            "istvan86/composer-focus-module-installer": true,
            "istvan86/composer-focus-theme-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
